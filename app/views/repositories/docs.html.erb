<% title_tag t(".Docs"), @repository.name %>

<%= render "menu" %>

<%= react_component "repositories/DocList", {
  repositoryId: @repository.id,
  newDocURL: @repository.to_path("/docs/new"),
  abilities: {
    update: can?(:update, Doc.new(repository_id: @repository.id)),
    destroy: can?(:destroy, Doc.new(repository_id: @repository.id)),
  },
} %>
