
<%= form_for([:admin,@share]) do |f| %>
  <%= render "/shared/error_messages", target: @share  %>
  
  <div class="form-group">
    <%= f.label :shareable, class: "control-label" %>
    <%= f.text_field :shareable, class: "form-control" %>
  </div>
  
  <div class="form-group">
    <%= f.label :user, class: "control-label" %>
    <%= f.text_field :user, class: "form-control" %>
  </div>
  
  <div class="form-action">
    <%= f.submit t("admin.Save"), class: "btn btn-primary mr-1" %> <%= link_to t("admin.Cancel"), admin_comments_path %>
  </div>
<% end %>
