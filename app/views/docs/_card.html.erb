<% if doc %>
  <div class="doc-card">
    <div class="d-inline-block mb-1 doc-title">
      <%= doc_title_tag(doc) %>
    </div>
    <div class="desc text-gray f6">
      <%= user_name_tag(doc.repository&.user) %> <span class="divider">/</span>
      <%= repository_name_tag(doc.repository) %>
    </div>
  </div>
<% end %>
