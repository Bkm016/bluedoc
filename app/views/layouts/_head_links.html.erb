<meta name="viewport" content="width=device-width">
<% if current_user %>
<meta name="current-user" value="<%= current_user.slug %>" />
<% end %>

<%= stylesheet_pack_tag 'app', 'data-turbolinks-track': 'reload' %>
<%= stylesheet_pack_tag 'front', 'data-turbolinks-track': 'reload' %>
<script src="//unpkg.com/jquery@3.3.1/dist/jquery.min.js"></script>
<script src="//unpkg.com/rails-ujs@5.2.1/lib/assets/compiled/rails-ujs.js"></script>
<script src="//unpkg.com/turbolinks@5.2.0/dist/turbolinks.js"></script>
<script src="//unpkg.com/react@16.2.0/umd/react.production.min.js"></script>
<script src="//unpkg.com/react-dom@16.2.0/umd/react-dom.production.min.js"></script>
<script src="//unpkg.com/react-dom@16.2.0/umd/react-dom-server.browser.production.min.js"></script>
<%= javascript_pack_tag 'vendors', 'data-turbolinks-track': 'reload' %>
<%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
