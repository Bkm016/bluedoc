<meta name="viewport" content="width=device-width">
<% if current_user %>
<meta name="current-user" value="<%= current_user.slug %>" />
<% end %>

<%= stylesheet_pack_tag 'app', 'data-turbolinks-track': 'reload' %>
<%= stylesheet_pack_tag 'front', 'data-turbolinks-track': 'reload' %>
<script type="text/javascript" src="/js/jquery.min.js"></script>
<script type="text/javascript" src="/js/rails-ujs.js"></script>
<script type="text/javascript" src="/js/turbolinks.js"></script>
<script type="text/javascript" src="/js/react.production.min.js"></script>
<script type="text/javascript" src="/js/react-dom.production.min.js"></script>
<script type="text/javascript" src="/js/react-dom-server.browser.production.min.js"></script>
<%= javascript_pack_tag 'vendors', 'data-turbolinks-track': 'reload' %>
<%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
