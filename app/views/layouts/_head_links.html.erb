<meta name="viewport" content="width=device-width">
<% if current_user %>
<meta name="current-user" value="<%= current_user.slug %>" />
<% end %>

<%= stylesheet_pack_tag 'app', 'data-turbolinks-track': 'reload' %>
<%= stylesheet_pack_tag 'front', 'data-turbolinks-track': 'reload' %>
<%= stylesheet_pack_tag 'print', 'data-turbolinks-track': 'reload', media: "print" %>
<script type="text/javascript" src="/js/jquery@3.3.1.js"></script>
<script type="text/javascript" src="/js/rails-ujs@5.2.1.js"></script>
<script type="text/javascript" src="/js/turbolinks@5.2.0.js"></script>
<script type="text/javascript" src="/js/react.production.min@16.6.1.js"></script>
<script type="text/javascript" src="/js/react-dom.production.min@16.6.1.js"></script>
<script type="text/javascript" src="/js/react-dom-server.browser.production.min@16.6.1.js"></script>
<%= javascript_pack_tag 'vendors', 'data-turbolinks-track': 'reload' %>
<%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
